  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:server": "NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite --config client/vite.config.ts --host",
    "dev": "concurrently -k --kill-others-on-fail --name \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm:dev:server\" \"npm:dev:client\"",
    "dev:shell": "./dev.sh",
    "dev:client:alt": "vite --config client/vite.config.ts --port 5174 --host",
    "dev:alt": "vite --config client/vite.config.alt.ts --host",
    "build:client": "vite build --config client/vite.config.ts",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server",
    "build": "npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push"
  },
